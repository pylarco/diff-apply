{
  "input": {
    "maxFileSize": 52428800
  },
  "output": {
    "filePath": "docs/repomix-output.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": false,
    "directoryStructure": true,
    "files": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": true,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 100
    }
  },
  "include": [
    "src/strategies/__tests__",
    // "src/strategies/new-unified/__tests__"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "docs",
      "repomix.config.json",
      "LICENSE",
      "bun.lockb",
      ".repomixignore",
      ".npmignore",
      ".gitignore",
      "node_modules",
      "README.md",
      // "src/strategies/__tests__",
      // "src/strategies/new-unified/__tests__"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}